{
    "name": "magmell-agentur/contao-validation-helpers",
    "description": "Contao 4 mini bundle to collect some validation helpers to be used in form fields",
    "type": "contao-bundle",
    "authors": [
        {
            "name": "Markus Milkereit",
            "email": "markus.milkereit@magmell.de",
            "role": "Developer"
        },
        {
            "name": "Marko TodiÄ‡",
            "email": "drexplosiveweb@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "contao/core-bundle": "^4.4",
        "globalcitizen/php-iban": "^2.7"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.4"
    },
    "extra": {
        "contao-manager-plugin": "Magmell\\Contao\\Validation\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Magmell\\Contao\\Validation\\": "src"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    }
}
